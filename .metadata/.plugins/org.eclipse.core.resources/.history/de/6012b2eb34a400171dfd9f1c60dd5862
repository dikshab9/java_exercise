package exercise5;
public class pe2_new {
	class Inner{
        void msg() {
            System.out.println("method inside inner class");              
        }
    }
    Inner disp() {
        Inner in = new Inner();
        System.out.println("method outside inner class");   
        return in;      
    }
    public static void main(String[] args) {
        pe2_new obj1 = new pe2_new();
            Inner in;
            in = obj1.disp();
            pe2_new.Inner obj2=obj1.new Inner();
            obj2.msg();
            
            
    }

}
