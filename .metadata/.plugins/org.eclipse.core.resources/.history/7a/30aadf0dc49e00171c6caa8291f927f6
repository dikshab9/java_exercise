package assignments;

class Member{
	String name;
	int age;
	double salary; 
}
class Member_a{
	public static void main(String args[]) {
		Member m=new Member();
		m.name="harry potter";
		m.age=32;
		m.salary=2000.93;
		System.out.println("Member name: " +m.name);
		System.out.println("Member age: " +m.age);
		System.out.println("Member salary: " +m.name);
	}
}
