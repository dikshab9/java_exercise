package exercise4;
import java.util.*;
 class EncapTest{
	private String name;
	private int id;
	private int age;
	public String getName() {
		return name;
	}
	
	public int getId() {
		return id;
	}
	
	public int getAge() {
		return age;
	}
	
	public void setName(String newname) {
		name=newname;
	}
	public void setAge(int newage) {
		age=newage;
	}
	public void setId(int newid) {
		id=newid;
	}
}
public class pe4 {
	public static void main(String[] args) {
		EncapTest encap=new EncapTest();
		encap.setName("diksha");
		encap.setAge(22);
		encap.setId(9);
		System.out.print("Name: "+ encap.getName()+"ID: "+ encap.getId()+"Age: "+ encap.getAge());
		
	}

}
